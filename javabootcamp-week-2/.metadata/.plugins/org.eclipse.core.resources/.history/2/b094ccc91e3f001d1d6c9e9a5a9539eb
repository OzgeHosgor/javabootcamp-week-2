package classes;
//Class Nedir? => Gruplama


public class Main {

	public static void main(String[] args) {
		//class'lar referans type
		CustomerMenager customerManager = new CustomerMenager();
		CustomerMenager customerMenager2 = new CustomerMenager();
		customerManager=customerMenager2;
		customerManager.Add();
		customerManager.Remove();
		customerManager.Update();
		
		//value
		int sayi1 =10;
		int sayi2 =20;
		sayi2=sayi1;
		sayi1=30;
		System.out.println(sayi2);
	}

}
